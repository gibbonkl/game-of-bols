            <div class="user-view">
                <div class="background gradient-sidenav"></div>
                <div class="input-field col s12">
                    <% if(user.imagem){ %>
                    <img class="circle resize-photo" src="../../public/uploads/<%=user.imagem %>">
                    <% } else { %>
                    <img class="circle avatar-user resize-photo" src="../../public/img/user.png" alt="">
                    <% } %>
                </div>
                <!-- caso usuário logado, mostra olá e o username no sidenav  -->
                <p><span class="white-text name <%if(user.tipo==''){%><%='hide'%><%}%>">Olá, <%=user.nome%>!</span></p>
                <!-- caso usuário não esteja logado, mostra só olá -->
                <p><span class="white-text name <%if(user.tipo=='user' || user.tipo=='admin'){%><%='hide'%><%}%>">Olá!</span></p>
            </div>
        </li>
        <li><a href="/">GAME OF BOLS</a></li>
